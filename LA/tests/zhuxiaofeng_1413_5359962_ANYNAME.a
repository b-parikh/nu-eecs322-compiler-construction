void main ( ){
	int64 %var1
	%var1 <- 7
	call print(%var1)
	int64 %var2
	%var2 <- call encode (%var1)
	call print(%var2)
	int64 %var3
	%var3 <- call decode (%var2)
	call print(%var3)
	return
}

int64 encode (int64 %var) {
     %var <- %var << 1
     %var <- %var + 1
     return %var
}

int64 decode (int64 %var) {
     %var <- %var >> 1
     return %var
}