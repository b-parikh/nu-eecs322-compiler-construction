    define :encode (data) {
        data <- data << 1
        data <- data + 1
        return data
    }

    // replace odd values in an array with 0
    define :filter_odd (vec_ptr) {
        size <- load vec_ptr
        size_e <- call :encode(size)
        call print(size_e)
        ptr <- vec_ptr
        
        :loop
        ptr < ptr + 8
        over <- size = 0
        br over :finish :cont

        :cont
        ele <- load ptr
        ele <- ele >> 1 // decode
        odd <- ele & 1
        br odd :filter :nop
        :filter
        store ptr <- 0
        :nop
        size <- size - 1
        br :loop

        :finish
        return
    }

